{
  "name": "n9-node-routing",
  "version": "2.1.0",
  "description": "Library to easily start app with routing-controllers",
  "keywords": [
    "node",
    "express",
    "decorators",
    "routing controllers",
    "typescript",
    "anotation"
  ],
  "license": "MIT",
  "main": "./dist/src/index.js",
  "type": "module",
  "exports": {
    "types": "./dist/src/index.d.ts",
    "default": "./dist/src/index.js"
  },
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "dev": "rimraf dist/ && tsc --watch",
    "build": "rimraf dist/ && tsc",
    "format": "prettier --write '{,@(src|test)/**/}*.*'",
    "lint": "eslint --config .eslintrc.yaml '{src,test}/**/*.ts'",
    "lint:apply": "npm run format && eslint --fix --config .eslintrc.yaml '{src,test}/**/*.ts'",
    "lint:verbose": "eslint --fix --config .eslintrc.yaml '{src,test}/**/*.ts' --format unix '{src,test}/**/*.ts'",
    "test:debug": "export NODE_ENV=test && TS_NODE_FILES=true ava debug --verbose --color --serial --host 0.0.0.0 --port 9230 --break",
    "test:dev": "export NODE_ENV=test && TS_NODE_FILES=true ava --verbose --color --serial --watch",
    "test": "npm run lint && npm run build && export NODE_ENV=test && nyc ava --verbose --color --serial --no-worker-threads && nyc report --reporter=html",
    "test:nolint": "npm run build && export NODE_ENV=test && nyc ava --verbose --color --serial --no-worker-threads && nyc report --reporter=html",
    "test:one": "npm run lint && npm run build && export NODE_ENV=test && nyc ava --verbose --color --serial",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -f coverage.lcov",
    "preversion": "npm test",
    "prepublishOnly": "npm run build",
    "release": "export BABEL_DISABLE_CACHE=1 && release-it --ci",
    "release:dry": "export BABEL_DISABLE_CACHE=1 && release-it --ci --dry-run=true",
    "post:release": "echo Successfully released $npm_package_version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "author": {
    "name": "Benjamin DANIEL",
    "email": "benjamin.daniel@neo9.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neo9/n9-node-routing.git"
  },
  "homepage": "https://github.com/neo9/n9-node-routing#n9-node-routing",
  "tags": [
    "express",
    "typescript-express",
    "routing-controllers"
  ],
  "engines": {
    "node": ">=16.0"
  },
  "dependencies": {
    "@benjd90/routing-controllers": "^0.9.2",
    "@neo9/n9-node-conf": "^1.4.1",
    "@neo9/n9-node-log": "4.1.0",
    "@neo9/n9-node-utils": "^2.2.1",
    "@promster/express": "^7.0.6",
    "@promster/server": "^7.0.8",
    "@sentry/node": "6.7.0",
    "@sentry/tracing": "6.7.0",
    "app-root-dir": "^1.0.2",
    "async": "^3.2.4",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "class-validator-jsonschema": "^3.0.1",
    "cls-hooked": "^4.2.2",
    "express": "^4.18.2",
    "fast-safe-stringify": "^2.1.1",
    "form-data": "^4.0.0",
    "glob": "^7.1.7",
    "glob-promise": "^4.2.0",
    "got": "^11.8.2",
    "helmet": "^4.6.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "3.6.6",
    "morgan": "1.10.0",
    "multer": "^1.4.2",
    "newrelic": "^8.10.0",
    "prom-client": "^14.1.0",
    "query-string": "^7.0.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers-openapi": "^4.0.0",
    "shortid": "^2.2.16",
    "swagger-ui-express": "^5.0.0",
    "typedi": "^0.10.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@commitlint/cli": "^17.7.1",
    "@neo9/n9-coding-style": "^5.1.2",
    "@neo9/n9-mongo-client": "^0.29.1",
    "@release-it/conventional-changelog": "^7.0.1",
    "@types/app-root-dir": "^0.1.1",
    "@types/async": "^3.2.20",
    "@types/cls-hooked": "^4.3.6",
    "@types/continuation-local-storage": "^3.2.5",
    "@types/express": "4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/helmet": "^4.0.0",
    "@types/lodash-es": "^4.17.9",
    "@types/mongodb": "^3.6.18",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/newrelic": "^7.0.4",
    "@types/node": "20.6.0",
    "@types/shortid": "^0.0.29",
    "@types/std-mocks": "^1.0.1",
    "@types/stream-array": "^1.1.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/url-join": "^4.0.1",
    "ava": "^5.3.1",
    "codecov": "^3.8.3",
    "conventional-changelog-cli": "^4.1.0",
    "eslint-plugin-import": "^2.28.1",
    "esm": "^3.2.25",
    "fs-extra": "^11.1.1",
    "husky": "^4.3.8",
    "mongodb-memory-server": "^8.15.1",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "std-mocks": "^1.0.1",
    "tmp-promise": "^3.0.3",
    "ts-node": "^10.9.1",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "timeout": "1m",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "require": [
      "ts-node/register",
      "reflect-metadata",
      "esm"
    ],
    "ignoredByWatcher": [
      "**/openapi-documentation.json"
    ]
  }
}
